<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".ui.programmercalculator.ProgrammerCalculatorFragment"> >

    <!-- TODO: Update blank fragment layout -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="test with new text" />
    </androidx.constraintlayout.widget.ConstraintLayout>

        <Button
            android:id="@+id/button0"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="120dp"
            android:background="@android:color/holo_blue_light"
            android:layout_toStartOf="@+id/button1"
            android:layout_marginEnd="5dp"
            android:text="0" />
        <Button
            android:id="@+id/button1"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/button0"
            android:layout_toStartOf="@+id/button2"

            android:layout_marginEnd="5dp"
            android:background="@android:color/holo_blue_light"
            android:text="1" />
        <Button
            android:id="@+id/button2"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/button1"
            android:layout_centerHorizontal="true"
            android:background="@android:color/holo_blue_light"
            android:text="2" />
        <Button
            android:id="@+id/button3"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/button0"
            android:layout_toEndOf="@+id/button2"

            android:layout_marginStart="5dp"
            android:background="@android:color/holo_blue_light"
            android:text="3" />
        <Button
            android:id="@+id/button4"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/button0"
            android:layout_alignEnd="@+id/button0"

            android:layout_marginTop="5dp"
            android:background="@android:color/holo_blue_light"
            android:text="4" />
        <Button
            android:id="@+id/button5"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/button4"
            android:layout_below="@+id/button1"
            android:layout_marginStart="5dp"
            android:layout_marginTop="5dp"
            android:background="@android:color/holo_blue_light"
            android:text="5" />
        <Button
            android:id="@+id/button6"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/button5"
            android:layout_below="@+id/button2"
            android:layout_marginStart="5dp"
            android:layout_marginTop="5dp"
            android:background="@android:color/holo_blue_light"
            android:text="6" />
        <Button
            android:id="@+id/button7"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/button6"
            android:layout_below="@+id/button3"
            android:layout_marginStart="5dp"
            android:layout_marginTop="5dp"
            android:background="@android:color/holo_blue_light"
            android:text="7" />

    <Button
        android:id="@+id/button8"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button4"
        android:layout_alignEnd="@+id/button4"

        android:layout_marginTop="5dp"
        android:background="@android:color/holo_blue_light"
        android:text="8" />
    <Button
        android:id="@+id/button9"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/button8"
        android:layout_below="@+id/button5"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:background="@android:color/holo_blue_light"
        android:text="9" />
    <Button
        android:id="@+id/buttonA"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/button9"
        android:layout_below="@+id/button6"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:background="@android:color/holo_blue_light"
        android:text="A" />
    <Button
        android:id="@+id/buttonB"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/buttonA"
        android:layout_below="@+id/button7"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:background="@android:color/holo_blue_light"
        android:text="B" />
    <Button
        android:id="@+id/buttonC"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button8"
        android:layout_alignEnd="@+id/button8"

        android:layout_marginTop="5dp"
        android:background="@android:color/holo_blue_light"
        android:text="C" />
    <Button
        android:id="@+id/buttonD"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/buttonC"
        android:layout_below="@+id/button9"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:background="@android:color/holo_blue_light"
        android:text="D" />
    <Button
        android:id="@+id/buttonE"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/buttonD"
        android:layout_below="@+id/buttonA"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:background="@android:color/holo_blue_light"
        android:text="E" />
    <Button
        android:id="@+id/buttonF"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/buttonE"
        android:layout_below="@+id/buttonB"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:background="@android:color/holo_blue_light"
        android:text="F" />

    <Button
        android:id="@+id/buttonsl"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button1"
        android:layout_alignStart="@+id/button0"
        android:background="@android:color/holo_blue_bright"
        android:layout_marginBottom="5dp"
        android:text="sl" />







</RelativeLayout>